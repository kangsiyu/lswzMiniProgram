/* pages/九鼎匹配/九鼎匹配分组.wxss */
page{
  width: 100%;
  height: 100%;
}
.tab_container {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.teamContainer{
  text-align: center;
}
.jixianCheckBoxOrigin{
  height: 200px;
  display: flex;
  flex-wrap: wrap;
  width: 100%;
}
.smallTeamCheckBox{
  height: 35px;
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  padding-bottom: 10px;
}
.jixianCheckOrigin{
  height: 25px;
  width: 30%;
  margin-right: 1%;
  margin-top: 4px;
  margin-left: 1%;
  text-align: center;
  background-color: #f0f0f0;
  border: 1px solid #f0f0f0;
  
}
.bingName{
  font-size: 15px;
}
.selectedCheck{
  border: 1px solid #fc6060;
  color: #fc6060;
}
.zhanduiContainer{
  width: 100%;
  height: 30px;
  display: flex;
  flex-direction: row;
}
.zhanduiTitle{
  width: 20%;
  line-height: 30px;
  text-align: center;
  border: solid 1px #f0f0f0;
  font-size: 12px;
}
.zhanduiName{
  width: 60%;
  line-height: 30px;
  text-align: center;
  border: solid 1px #f0f0f0;
  font-size: 12px;
}